{
    "ver": "0.0.1",
    "authors": [
        "asedova"
    ],
    "contact": "help@kbase.us",
    "visible": true,
    "categories": ["active","comparative_genomics"],
    "app_type": "editor",
    "widgets": {
        "input": "kbaseNarrativeMethodInput",
        "output": "no-display"
    },
    "parameters": [
        {
            "id": "msa_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options" : {
               "valid_ws_types" : [
                  "KBaseTree.MSA"
               ]
            }
        },
        {
            "id": "pdb_ref",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options" : {
               "valid_ws_types" : [
                  "KBaseStructure.ModelProteinStructure",
                  "KBaseStructure.ExperimentalProteinStructure"
               ]
            }
        },
        {
            "id": "max_gaps_per_position",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "100" ],
            "field_type": "text",
            "text_options" : {
               "validate_as" : "int",
               "min_int" : 0,
               "max_int" : 100
            }
        },
        {
            "id": "max_gaps_per_sequence",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "100" ],
            "field_type": "text",
            "text_options" : {
               "validate_as" : "int",
               "min_int" : 0,
               "max_int" : 100
            }
        },
        {
            "id": "contactmap_name",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options" : {
               "is_output_name" : true,
               "valid_ws_types" : [
                  "KBaseMatrices.ResidueContactMap"
               ]
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "contact_mapping",
            "method": "run_contact_mapping",
            "input_mapping": [
                {
                    "input_parameter": "msa_ref",
                    "target_property": "msa_ref",
                    "target_type_transform": "resolved-ref"
                },
                {
                    "input_parameter": "pdb_ref",
                    "target_property": "pdb_ref",
                    "target_type_transform": "resolved-ref"
                },
                {
                    "input_parameter": "max_gaps_per_position",
                    "target_property": "max_gaps_per_position"
                },
                {
                    "input_parameter": "max_gaps_per_sequence",
                    "target_property": "max_gaps_per_sequence"
                },
                {
                    "input_parameter": "contactmap_name",
                    "target_property": "contactmap_name"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                }
            ],
            "output_mapping" : [
                {
                    "service_method_output_path": [0, "report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0, "report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                }
         ]
        }
    },
    "job_id_output_field": "docker"
}